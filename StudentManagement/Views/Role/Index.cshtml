@model List<IdentityRole>
@{
    ViewData["Title"] = "Show All Roles";
}

<h1>Show All Roles</h1>

<table class="table mt-5 table-bordered table-hover">
    <thead>
        <tr>
            <th class="text-center text-center-sm">Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cus in Model)
        {
            <tr>
                <td class="text-center text-center-sm">@cus.Id</td>
                <td>@cus.Name</td>
            </tr>
        }
    </tbody>
</table>

